{
  "name": "nebenan.de",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "Chrome notify extension for nebenan.de",
  "icons": {
    "16": "images/icon-read_16.png",
    "128": "images/icon_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/builds/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "cookies",
    "notifications",
    "alarms",
    "https://api.nebenan.de/*",
    "https://nebenan.de/"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "omnibox": {
    "keyword": "nebenan"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-read_16.png",
      "128": "images/icon_128.png"
    },
    "default_title": "nebenan.de",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "images/icon_128.png",
    "images/thumb_dummy.jpg"
  ]
}
