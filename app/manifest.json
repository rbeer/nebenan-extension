{
  "name": "nebenan.de",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome notify extension for nebenan.de",
  "icons": {
    "16": "images/icon-read_16.png",
    "128": "images/icon_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/build/all.js"
    ]
  },
  "permissions": [
    "tabs",
    "cookies",
    "notifications",
    "https://api.nebenan.de/*",
    "https://nebenan.de/"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "omnibox": {
    "keyword": "nebenan"
  },
  "web_accessible_resources": [
    "images/icon_128.png"
  ]
}
