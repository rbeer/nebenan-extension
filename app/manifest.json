{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-read_16.png",
    "128": "images/icon_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "cookies",
    "notifications",
    "https://api.nebenan.de/*",
    "*://*.google.com/"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "omnibox": {
    "keyword": "OMNIBOX-KEYWORD"
  },
  "web_accessible_resources": [
    "images/icon_128.png"
  ]
}
